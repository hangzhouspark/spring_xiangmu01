<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hopu.mapper.ExportProCMapper">

    <insert id="insertexport" parameterType="com.hopu.entity.ExportProC">
        insert  into EXPORT_PRODUCT_C(EXPORT_PRODUCT_ID,EXPORT_ID,PRODUCT_NO,PACKING_UNIT,CNUMBER,BOX_NUM,PRICE) values (#{EXPORT_PRODUCT_ID},#{export_Id},#{product_No},#{packing_Unit},#{cnumber},#{box_Num},#{price})
    </insert>

</mapper>